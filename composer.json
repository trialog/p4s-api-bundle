{
	"name" : "trialog/p4s-api-bundle",
	"description" : "Un bundle PHP / Symfony2 permettant une intégration facile de l'API du P4S - Projet AMISURE",
	"type" : "library",
	"authors": [
		{
			"name": "Olivier Maridat (Trialog)",
			"homepage": "http://www.trialog.com"
		}
	],
	"license" : [
		"LGPL-3"
	],
	"require" : {
		"symfony/symfony" : "2.3.*",
		"doctrine/doctrine-fixtures-bundle" : "dev-master",
		"doctrine/orm" : ">=2.2.3,<2.4-dev",
		"symfony/assetic-bundle" : "2.3.*",
		"twig/extensions" : "1.0.*",
		"hwi/oauth-bundle" : "0.3.*@dev",
		"php" : ">=5.3.3",
		"sensio/generator-bundle" : "2.3.*",
		"symfony/monolog-bundle" : "2.3.*",
		"sensio/framework-extra-bundle" : "2.3.*",
		"sensio/distribution-bundle" : "2.3.*",
		"doctrine/doctrine-bundle" : "1.2.*"
	},
	"autoload" : {
		"psr-0" : {
			"" : "src/"
		}
	},
	"minimum-stability" : "stable",
	"config" : {
		"bin-dir" : "bin"
	},
}